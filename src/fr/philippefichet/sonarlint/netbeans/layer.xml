<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="SonarLint"
                  url="nbresloc:/fr/philippefichet/sonarlint/netbeans/sonarlint-annotation.xml">
                <attr name="requireProject" boolvalue="false"/>
            </file>
        </folder>
    </folder>
    <folder name="TaskList">
        <folder name="Groups">
            <file name="sonarlint.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.tasklist.Task.createGroup"/>
                <attr name="localizingBundle" stringvalue="fr.philippefichet.sonarlint.netbeans.Bundle"/>
                <attr name="groupName" stringvalue="nb-sonarlint"/>
                <attr name="diplayNameKey" stringvalue="LBL_sonarlint_group"/>
                <attr name="descriptionKey" stringvalue="HINT_sonarlint_group"/>
                <attr name="iconKey" stringvalue="ICON_whitelist"/>
                <attr name="position" intvalue="150"/>
            </file>
        </folder>
        <folder name="Scanners">
            <file name="SonarLintTaskScanner.instance">
                <attr name="instanceOf" stringvalue="org.netbeans.spi.tasklist.FileTaskScanner"/>
                <attr name="instanceCreate" methodvalue="fr.philippefichet.sonarlint.netbeans.SonarLintTaskScanner.create"/>
            </file>
        </folder>
    </folder>
    <folder name="OptionsExport">
        <folder name="Java">
            <file name="SonarLint">
                <attr name="displayName" bundlevalue="fr.philippefichet.sonarlint.netbeans.Bundle#OptionsExport.displayName"/>
                <attr name="include" stringvalue="config/Preferences/fr/philippefichet/sonarlint/.*"/>
            </file>
        </folder>
    </folder>
</filesystem>
